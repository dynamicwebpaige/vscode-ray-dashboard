{
  "name": "vscode-ray-dashboard",
  "displayName": "Ray",
  "description": "Ray for VS Code",
  "version": "0.0.1",
  "author": {
    "name": "Paige Bailey"
  },
  "publisher": "dynamicwebpaige",
  "license": "See LICENSE in LICENSE",
  "repository": {
    "url": "https://github.com/dynamicwebpaige/vscode-ray-dashboard"
  },
  "engines": {
    "vscode": "^1.57.0-insider"
  },
  "categories": [
    "Data Science"
  ],
  "activationEvents": [
    "onCommand:ray.connectToDashboard"
  ],
  "main": "./out/extension/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ray.connectToDashboard",
        "title": "Connect to Dashboard",
        "category": "Ray"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile && node out/test/checkNoTestProvider.js",
    "compile": "npm run compile:extension && npm run compile:client",
    "compile:extension": "tsc -b",
    "compile:client": "webpack --mode production",
    "lint": "eslint src --ext ts",
    "watch": "concurrently -r \"npm:watch:*\"",
    "watch:extension": "tsc -b --watch",
    "watch:client": "webpack --mode development --watch",
    "dev": "npm run compile && concurrently -r npm:watch:extension npm:dev:client",
    "dev:client": "webpack serve",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.57.0",
    "@types/vscode-notebook-renderer": "^1.48.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "concurrently": "^5.3.0",
    "css-loader": "^4.2.0",
    "eslint": "^7.19.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-dts": "^0.3.1",
    "vscode-notebook-error-overlay": "^1.0.1",
    "vscode-test": "^1.5.0",
    "webpack": "^5.37.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@webpack-cli/serve": "^1.4.0",
    "ansicolor": "^1.1.93"
  }
}
